<!-- sections/banner-collection.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-banner.css' | asset_url | stylesheet_tag }}
{{ 't4s-collection-products.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
    assign sid = section.id
    assign se_stts = section.settings
    assign stt_layout = se_stts.layout
    assign stt_image_bg = se_stts.image_bg
    assign countdown = false
    assign general_block = false
    assign newsletter = false
    if stt_layout == 't4s-se-container' 
        assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
    elsif stt_layout == 't4s-container-wrap'
        assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
    else
        assign html_layout = '__' | split: '__'
    endif
    assign image_ratio = se_stts.image_ratio
    if image_ratio == "ratioadapt"
        assign imgatt = ''
    else 
        assign imgatt = 'data-'
    endif
    assign collection = collections[se_stts.collection]
    assign show_vendor = se_stts.show_vendor
    assign use_link_vendor = settings.use_link_vendor
    assign enable_rating = settings.enable_rating
    assign limit = se_stts.limit
    assign product_des = se_stts.product_des
    if se_stts.btn_owl == "outline"
        assign arrow_icon = 1
    else
        assign arrow_icon = 2
    endif

    assign show_img = settings.show_img
    assign isGrowaveWishlist = false
    if settings.wishlist_mode == "3" and shop.customer_accounts_enabled
        assign isGrowaveWishlist = true
    endif
    assign enable_pr_size = settings.enable_pr_size
    assign pr_size_pos = settings.pr_size_pos
    assign show_size_type = settings.show_size_type
    assign size_ck = settings.size_ck | append: ',size,sizes,Größe' 
    assign get_size = size_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

    assign enable_pr_color = settings.enable_pr_color
    assign show_cl_type = settings.show_color_type
    assign color_ck = settings.color_ck | append: ',color,colors,couleur,colour'
    assign get_color = color_ck | downcase | replace: ' ,', ',' | replace: ', ', ',' | split: ',' | uniq

    assign price_varies_style = settings.price_varies_style
    assign app_review = settings.app_review
    assign use_countdown = se_stts.use_cdt

    assign countdown = false
    assign use_button = false
    assign use_animation = false
    assign general_block = false
    assign no_block_parent = true

    if se_stts.coll_pos == 'left'
        assign order_1 = ' t4s-order-lg-1'
        assign order_2 = ' t4s-order-lg-12'
    endif
 -%}
<div class="t4s-section-inner t4s_nt_se_{{ sid }} {{ stt_layout }} {%- if stt_image_bg != blank and stt_layout != 't4s-se-container' -%} t4s-has-imgbg lazyloadt4s {%- endif -%}" {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto" data-optimumx="2" {% endif %} {% render 'section_style', se_stts: se_stts %}>
    {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner {% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s {% endif %}" {% if stt_image_bg != blank %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="2" {% endif %} > {%- endif -%}
    {%- render 'section_tophead', se_stts: se_stts -%}
    <div data-collection-url="{{ collection.url }}" class="t4s-row t4s-g-0 t4s-row-cols-lg-2 t4s-row-cols-md-1 t4s-row-cols-1 t4s-products-border__{{ se_stts.enable_border }}">
        {%- for block in section.blocks -%}
            {%- assign block_type = block.type -%}
            {%- if block_type == 'b_item' -%} 
                {%- liquid 
                    unless no_block_parent
                        echo '</div></div></div></div>'
                    endunless
                    assign no_block_parent = false  
                    assign bk_stts = block.settings
                    assign bg_content_op = bk_stts.bg_content_op | divided_by: 100.0 
                    assign bg_content = bk_stts.bg_content_cl | color_modify: 'alpha', bg_content_op 
                    assign bg_opacity = bk_stts.bg_opacity | divided_by: 100.0
                    assign bg_overlay = bk_stts.bg_overlay | color_modify: 'alpha', bg_opacity
                    assign br_opacity = bk_stts.br_opacity | divided_by: 100.0
                    assign br_bg = bk_stts.br_bg | color_modify: 'alpha', br_opacity
                    assign ani_delay = 0   
                    assign percent_delay = bk_stts.animation_delay | divided_by: 100.0
                    assign time_ani_delay = bk_stts.time_animation | times: percent_delay  
                    assign image_mb = bk_stts.image_mb
                    assign image = bk_stts.image | default: image_mb
                    assign mb_image = image_mb | default: image
                    if bk_stts.b_link != blank
                        assign ARRhtml = 'a,,' | split: ','
                    else
                        assign ARRhtml = 'div,data-,data-' | split: ','
                    endif
               -%}
                <div class="t4s-col-item t4s-banner-collection-img{{ order_1 }} t4s_cover" timeline hdt-reveal="slide-in">
                    <div data-t4s-animate class="t4s-banner-item t4s-eff t4s-eff-{{ bk_stts.b_effect }} t4s-eff-img-{{ bk_stts.img_effect }} t4s_ratio_mix t4s_ratioadapt_mix t4s_ratio_cuspx_mb_{{ bk_stts.custom_mb }} t4s_ratio_cuspx_tb_{{ bk_stts.custom_tb }} t4s_ratio_cuspx_{{ bk_stts.custom_dk }}" style="--ratioapt: {{ image.aspect_ratio | default: 1.5 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.5 }};--aspect-ratio-cus : {{ bk_stts.height_dk }}px;--aspect-ratio-custb : {{ bk_stts.height_tb }}px;--aspect-ratio-cusmb : {{ bk_stts.height_mb }}px;">
                        <div class="t4s-banner-inner lazyloadt4s t4s-h-100" style="--bg-overlay: {{ bg_overlay }};">
                            {%- if bk_stts.bg_content_bl -%}
                                {%- capture append_bg_content_style -%}--bg-content:{{ bg_content }};--content-pd:{{ bk_stts.content_pd_tb }}px {{ bk_stts.content_pd_lr }}px;--content-pd-mb:{{ bk_stts.content_pd_tb_mb }}px {{ bk_stts.content_pd_lr_mb }}px;{%- endcapture -%}
                            {%- endif -%}
                            {%- if bk_stts.border_bl -%}
                                {%- capture append_bg_border_style -%}--br-color:{{ bk_stts.br_color }};--br-style:{{ bk_stts.br_style }};--br-pd:{{ bk_stts.br_pd }}px;--br-pd-mb:{{ bk_stts.br_pd_mb }}px;--border-bg:{{ br_bg }};{%- endcapture -%}
                            {%- endif -%}
                            <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ bk_stts.b_link }}" {{ ARRhtml[2] }}target="{{ bk_stts.open_link }}" class="t4s-d-block t4s_ratio t4s_ratio_hasmb {% unless block.settings.custom_dk %} t4s-h-100{% endunless %}">
                            {%- if image != blank -%}
                                <img {% if mb_image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ mb_image.presentation.focal_point }}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-md-none" data-src="{{ mb_image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: mb_image.width, h: mb_image.height %}" width="{{ mb_image.width }}" height="{{ mb_image.height }}" alt="{{ mb_image.alt | escape }}" loading="lazy">
                                <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: url({{ mb_image | image_url: width: 1 }})"></span>
                                <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ image.presentation.focal_point }}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-d-none t4s-d-md-block" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                                <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: url({{ image | image_url: width: 1 }})"></span>
                            {%- else -%}
                                {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg2 t4s-obj-eff' }}
                            {%- endif -%}
                            </{{ ARRhtml[0] }}>
                            <div class="t4s-banner-content t4s-content-position t4s-pa t4s-content-auto t4s-pe-none t4s-text-lg-{{ bk_stts.content_align }} t4s-text-md-{{ bk_stts.content_align_tb }} t4s-text-{{ bk_stts.content_align_mb }} t4s-bg-content-{{ bk_stts.bg_content_bl }} t4s-br-content-{{ bk_stts.border_bl }} t4s-br-style-{{ bk_stts.br_style }}" style="--time-animation:{{ bk_stts.time_animation }}s; {%- render 'content_position', ch_pos: bk_stts.ch_pos, cv_pos: bk_stts.cv_pos, ch_pos_tb: bk_stts.ch_pos_tb, cv_pos_tb: bk_stts.cv_pos_tb, ch_pos_mb: bk_stts.ch_pos_mb, cv_pos_mb: bk_stts.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%}">
            {%- else -%} 
                {%- liquid          
                    if no_block_parent
                        continue
                    endif
                    assign bk_stts = block.settings 
               -%}       
                    {%- case block.type -%}
                        {%- when 'custom_text' -%}
                                {% if bk_stts.text != blank %}
                                    {%- assign general_block = true -%}
                                    <{{ bk_stts.tag }} class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-text-bl t4s-fnt-fm-{{ bk_stts.fontf }} t4s-font-italic-{{ bk_stts.font_italic }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-hidden-br-tb-{{ bk_stts.remove_br_tag_tb }} t4s-hidden-br-mb-{{ bk_stts.remove_br_tag_mb }} t4s-text-shadow-{{ bk_stts.text_shadow }}" data-lh="{{ bk_stts.text_lh_mb }}" data-lh-md="{{ bk_stts.text_lh_tb }}" data-lh-lg="{{ bk_stts.text_lh }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'custom_text', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.text }}</{{ bk_stts.tag }}>
                                {% endif %}
                            {%- when 'space_html' -%}
                                {%- assign general_block = true -%}
                                <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-space-html t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', bk_stts: bk_stts, ani_delay: ani_delay -%}></div>
                        {%- when 'html' -%}
                            {%- if bk_stts.html_content != blank -%}
                                {%- assign general_block = true -%}
                                    <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-raw-html t4s-rte--list t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {%- render 'bk_cus_style', type: 'html', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.html_content }}</div>
                                {%- endif -%}
                        {%- when 'image' -%}
                            {%- assign image = bk_stts.image_child -%}
                            {%- if image != blank -%}
                                {%- assign general_block = true -%}
                                {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-img-child t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                    <img data-maxw="{{ bk_stts.img_width_mb }}" data-maxw-md="{{ bk_stts.img_width_tb }}" data-maxw-lg="{{ bk_stts.img_width }}" class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                                </div>
                            {%- endif -%}
                        {%- when "countdown" -%}
                            {%- if bk_stts.date != blank -%}
                                {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                {%- assign countdown = true -%}
                                <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-countdown t4s-countdown-des-{{ bk_stts.cdt_des }} t4s-countdown-size-{{ bk_stts.cdt_size }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'countdown', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                    <div class="t4s-time" data-countdown-t4s data-date='{{ bk_stts.date }}' data-keyid='#countdown-{{ sid }}'></div>
                                </div>
                            {%- endif -%}
                        {%- when 'custom_button' -%}
                            {%- if bk_stts.button_link != blank and bk_stts.button_text != blank -%}
                                {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                                {%- assign use_button = true -%}
                                {%- assign button_style = bk_stts.button_style -%}
                                <a href="{{ bk_stts.button_link }}" target="{{ bk_stts.target_link }}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ bk_stts.fontf }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ bk_stts.button_effect }}{% endif %}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'custom_button', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.button_text }}
                                    {%- if bk_stts.button_icon_w > 0 -%}<svg  class="t4s-btn-icon" viewBox="0 0 14 10"><use xlink:href="#t4s-icon-btn"></use></svg>{%- endif -%}
                                </a>
                            {%- endif -%}
                        {%- when 'newsletter' -%}
                            {%- assign newsletter = true -%}
                            <div id="b_{{ block.id }}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-pe-auto t4s-newsletter-parent t4s-newsl-des-{{ bk_stts.newl_des }} t4s-newsl-{{ bk_stts.newl_size }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-text-start" {%- render 'bk_cus_style', type: 'newsletter', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                {%- render 'newsletter', form_id: block.id, buttonIcon: bk_stts.btn_icon, bk_stts: bk_stts -%}
                            </div>
                    {%- endcase -%}
                    {%- if bk_stts.animation != 'none' -%}{%- assign ani_delay = ani_delay | plus: time_ani_delay -%}{%- endif -%}
                {%- endif -%}
            {%- liquid
                if forloop.last
                    echo '</div></div></div></div>'
                endif -%}          
        {%- endfor -%}
    <div class="t4s-col-item t4s-banner-collection-prs{{ order_2 }}{% if se_stts.layout_des == "1" %} t4s-d-flex t4s-align-items-center t4s-justify-content-center t4s-flex-column{% endif %}">
          {%- if se_stts.layout_des == "1" -%} 
            <div class="t4s_box_pr_grid t4s-w-100 t4s-products t4s-justify-content-center t4s-text-{{ se_stts.content_align }} t4s_{{ image_ratio }} t4s_{{ se_stts.image_size }} t4s-row t4s-row-cols-lg-{{ se_stts.col_dk }} t4s-row-cols-md-{{ se_stts.col_tb }} t4s-row-cols-{{ se_stts.col_mb }} t4s-gx-lg-{{ se_stts.space_h_item }} t4s-gy-lg-{{ se_stts.space_v_item }} t4s-gx-md-{{ se_stts.space_h_item_tb }} t4s-gy-md-{{ se_stts.space_v_item_tb }} t4s-gx-{{ se_stts.space_h_item_mb }} t4s-gy-{{ se_stts.space_v_item_mb }}">
           {%- else -%} 
            <div data-t4s-resizeobserver class="t4s-flicky-slider t4s_box_pr_slider t4s-products t4s-text-{{ se_stts.content_align }} t4s_{{ image_ratio }} t4s_{{ se_stts.image_size }} {% if se_stts.nav_btn %} t4s-slider-btn-style-{{ se_stts.btn_owl }} t4s-slider-btn-{{ se_stts.btn_shape }} t4s-slider-btn-{{ se_stts.btn_size }} t4s-slider-btn-cl-{{ se_stts.btn_cl }} t4s-slider-btn-vi-{{ se_stts.btn_vi }} t4s-slider-btn-hidden-mobile-{{ se_stts.btn_hidden_mobile }} {% endif %} {% if se_stts.nav_dot == true %} t4s-dots-style-{{ se_stts.dot_owl }} t4s-dots-cl-{{ se_stts.dots_cl }} t4s-dots-round-{{ se_stts.dots_round }} t4s-dots-hidden-mobile-{{ se_stts.dots_hidden_mobile }} {% endif %} t4s-row t4s-row-cols-lg-{{ se_stts.col_dk }} t4s-row-cols-md-{{ se_stts.col_tb }} t4s-row-cols-{{ se_stts.col_mb }} t4s-gx-lg-{{ se_stts.space_h_item }} t4s-gy-lg-{{ se_stts.space_v_item }} t4s-gx-md-{{ se_stts.space_h_item_tb }} t4s-gy-md-{{ se_stts.space_v_item_tb }} t4s-gx-{{ se_stts.space_h_item_mb }} t4s-gy-{{ se_stts.space_v_item_mb }} flickityt4s" data-flickityt4s-js='{"setPrevNextButtons":true,"arrowIcon":"{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": {{ se_stts.loop }},"prevNextButtons": {{ se_stts.nav_btn }},"percentPosition": 1,"pageDots": {{ se_stts.nav_dot }}, "autoPlay" : {{ se_stts.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ se_stts.au_hover }} }' style="--space-dots: {{ se_stts.dots_space }}px;">
         {%- endif -%} 
            {%- if collection != blank -%}
              {%- case product_des -%}
                {%- when '1' -%} 
                  {%- paginate collection.products by limit -%}
                    {%- render 'product-grid-item1' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                {%- endpaginate -%}
                {%- when '2' -%}
                  {%- paginate collection.products by limit -%}
                    {%- render 'product-grid-item2' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                  {%- endpaginate -%}
                {%- when '3' -%}
                  {%- paginate collection.products by limit -%}
                    {%- render 'product-grid-item3' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                  {%- endpaginate -%}
                {%- when '4' -%}
                  {%- paginate collection.products by limit -%}
                    {%- render 'product-grid-item4' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                  {%- endpaginate -%}
                {%- when '5' -%}
                  {%- paginate collection.products by limit -%}
                    {%- render 'product-grid-item5' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                  {%- endpaginate -%}
                {%- when '6' -%}
                    {%- paginate collection.products by limit -%}
                        {%- render 'product-grid-item6' for collection.products as product, isGrowaveWishlist: isGrowaveWishlist, show_img: show_img, show_vendor: show_vendor, use_link_vendor: use_link_vendor, enable_pr_color: enable_pr_color, show_cl_type: show_cl_type, get_color: get_color, enable_pr_size: enable_pr_size, pr_size_pos: pr_size_pos, get_size: get_size, show_size_type: show_size_type, price_varies_style: price_varies_style, app_review: app_review, enable_rating: enable_rating, use_countdown: use_countdown, show_list_btns: false -%}
                    {%- endpaginate -%}
