                            <h4 class="t4s-ins-title">{{ bk_stts.title }}</h4>
                        {%- endif -%}
                    </div>
                </div>
            {%- endif -%}
            <div class="t4s-row t4s_{{ se_stts.image_ratio }} t4s_{{ se_stts.image_size }} t4s-row-cols-lg-{{ se_stts.col_dk }} t4s-row-cols-md-{{ se_stts.col_tb }} t4s-row-cols-{{ se_stts.col_mb }} t4s-g-lg-{{ se_stts.space_item }} t4s-g-md-{{ se_stts.space_item_tb }} t4s-g-{{ se_stts.space_item_mb }} {% if se_stts.layout_des == '2' %}{% if se_stts.nav_btn == true %}  t4s-slider-btn-style-{{ se_stts.btn_owl }} t4s-slider-btn-{{ se_stts.btn_shape }} t4s-slider-btn-{{ se_stts.btn_size }} t4s-slider-btn-cl-{{ se_stts.btn_cl }} t4s-slider-btn-vi-{{ se_stts.btn_vi }} t4s-slider-btn-hidden-mobile-{{ se_stts.btn_hidden_mobile }} {% endif %}{% if se_stts.nav_dot %} t4s-dots-style-{{ se_stts.dot_owl }} t4s-dots-cl-{{ se_stts.dots_cl }} t4s-dots-round-{{ se_stts.dots_round }} t4s-dots-hidden-mobile-{{ se_stts.dots_hidden_mobile }} {% endif %} t4s-flicky-slider flickityt4s flickityt4s-later{% endif %}" data-inst4s-options='{ "id":"{{ se_stts.col_dk }}{{ se_stts.col_tb }}{{ se_stts.col_mb }}{{ se_stts.limit }}{{ se_stts.open_link }}","limit":{{ limit }}, "acc": {{ acc | base64_encode | json }}, "target":"{{ se_stts.open_link }}" }' {% if se_stts.layout_des == '2' %} data-flickityt4s-js='{"setPrevNextButtons":true,"arrowIcon":"{{ arrow_icon }}","imagesLoaded": 0,"adaptiveHeight": 0, "contain": 1, "groupCells": "100%", "dragThreshold" : 5, "cellAlign": "left","wrapAround": {{ se_stts.loop }},"prevNextButtons": {{ se_stts.nav_btn }},"percentPosition": 1,"pageDots": {{ se_stts.nav_dot }}, "autoPlay" : {{ se_stts.au_time | times: 1000 }}, "pauseAutoPlayOnHover" : {{ se_stts.au_hover }} }'  style="--space-dots: {{ se_stts.dots_space }}px;"{% endif %}><div class="t4s-loading--bg"></div></div>
