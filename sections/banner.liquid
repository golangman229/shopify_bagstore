<!-- section/banner.liquid -->
{{ 't4s-section.css' | asset_url | stylesheet_tag }}
{{ 't4s-content-position.css' | asset_url | stylesheet_tag }}
{{ 't4s-banner.css' | asset_url | stylesheet_tag }}
<link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- liquid
    assign sid = section.id
    assign se_stts = section.settings
    assign se_blocks = section.blocks
    assign stt_layout = se_stts.layout
    assign stt_image_bg = se_stts.image_bg
    assign countdown = false
    assign general_block = false
    assign newsletter = false
    assign no_block_parent = true

    if stt_layout == 't4s-se-container' 
        assign html_layout = '<div class="t4s-container">__</div></div>' | split: '__'
    elsif stt_layout == 't4s-container-wrap'
        assign html_layout = '<div class="t4s-container">__</div>' | split: '__'
    else
        assign html_layout = '__' | split: '__'
    endif
    assign countdown = false
    assign use_button = false
    assign use_animation = false
    assign general_block = false
 -%} 
<div class="t4s-section-inner t4s_nt_se_{{ sid }} {{ stt_layout }} {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank and stt_layout != 't4s-se-container' %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %} {% render 'section_style', se_stts: se_stts -%} >
    {{- html_layout[0] -}}
    {%- if stt_layout == 't4s-se-container' -%}<div class="t4s-container-inner{% if stt_image_bg != blank %} t4s-has-imgbg lazyloadt4s{% endif %}" {% if stt_image_bg != blank %} data-bgset="{{ stt_image_bg | image_url: width: 1 }}" data-sizes="auto"  data-optimumx="1.5"{% endif %}>{% endif -%}
    {%- render 'section_tophead', se_stts: se_stts -%}
    <div class="t4s-banner-holder isotopet4s t4s-row t4s-gx-lg-{{ se_stts.space_h_item }} t4s-gy-lg-{{ se_stts.space_v_item }} t4s-gx-md-{{ se_stts.space_h_item_tb }} t4s-gy-md-{{ se_stts.space_v_item_tb }} t4s-gx-{{ se_stts.space_h_item_mb }} t4s-gy-{{ se_stts.space_v_item_mb }}" data-isotopet4s-js='{ "itemSelector": ".t4s-banner-wrap", "layoutMode": "packery" }'>
    {%- for block in se_blocks -%}
        {%- assign block_type = block.type -%}
        {%- if block_type == 'b_item' -%} 
            {%- liquid
            unless no_block_parent
                echo '</div></div></div></div>'
            endunless
            assign no_block_parent = false        
            assign bk_stts = block.settings
            assign image_mb = bk_stts.image_mb
            assign image = bk_stts.image | default: image_mb
            assign mb_image = image_mb | default: image
            assign b_link = bk_stts.b_link
            if b_link != blank 
                assign ARRhtml = 'a,,' | split: ','
            else
                assign ARRhtml = 'div,data-,data-' | split: ','
            endif
            assign bg_content_op = bk_stts.bg_content_op | divided_by: 100.0 
            assign bg_content = bk_stts.bg_content_cl | color_modify: 'alpha', bg_content_op 
            assign bg_opacity = bk_stts.bg_opacity | divided_by: 100.0
            assign bg_overlay = bk_stts.bg_overlay | color_modify: 'alpha', bg_opacity
            assign br_opacity = bk_stts.br_opacity | divided_by: 100.0
            assign br_bg = bk_stts.br_bg | color_modify: 'alpha', br_opacity   
            assign ani_delay = 0   
            assign percent_delay = bk_stts.animation_delay | divided_by: 100.0
            assign time_ani_delay = bk_stts.time_animation | times: percent_delay  
           -%}
        <div class="t4s-isotopet4s-item t4s-col-item t4s-banner-wrap t4s_{{ bk_stts.image_size }} t4s-col-lg-{{ bk_stts.col_dk }} t4s-col-md-{{ bk_stts.col_tb }} t4s-col-{{ bk_stts.col_mb }}"{% if bk_stts.col_dk == 'custom' or bk_stts.col_tb == 'custom' %} style="--t4s-cus-col-lg:{{ bk_stts.col_dk_w | remove: ' ' }}%;--t4s-cus-col-md:{{ bk_stts.col_dk_w_tb | remove: ' ' }}%;"{% endif %}>
          <div data-t4s-animate class="t4s-banner-item t4s-eff t4s-eff-{{ bk_stts.b_effect }} t4s-eff-img-{{ bk_stts.img_effect }} t4s_ratio_mix t4s_ratio_cuspx_mb_{{ bk_stts.custom_mb }} t4s_ratio_cuspx_tb_{{ bk_stts.custom_tb }} t4s_ratio_cuspx_{{ bk_stts.custom_dk }}" style="--ratioapt: {{ image.aspect_ratio | default: 1.5 }};--ratioaptmb: {{ mb_image.aspect_ratio | default: 1.5 }};--aspect-ratio-cus : {{ bk_stts.height_dk }}px;--aspect-ratio-custb : {{ bk_stts.height_tb }}px;--aspect-ratio-cusmb : {{ bk_stts.height_mb }}px;">
            <div class="t4s-banner-inner" style="--bg-overlay: {{ bg_overlay }};" timeline hdt-reveal="slide-in">
                {%- if bk_stts.bg_content_bl -%}
                    {%- capture append_bg_content_style -%}--bg-content: {{ bg_content }};--content-pd: {{ bk_stts.content_pd_tb }}px {{ bk_stts.content_pd_lr }}px;--content-pd-tb: {{ bk_stts.content_pd_tb_tb }}px {{ bk_stts.content_pd_lr_tb }}px;--content-pd-mb: {{ bk_stts.content_pd_tb_mb }}px {{ bk_stts.content_pd_lr_mb }}px;{%- endcapture -%}
                {%- endif -%}
                {%- if bk_stts.border_bl -%}
                    {%- capture append_bg_border_style -%}--br-color: {{ bk_stts.br_color }};--br-style: {{ bk_stts.br_style }};--br-pd: {{ bk_stts.br_pd }}px;--br-pd-tb: {{ bk_stts.br_pd_tb }}px;--br-pd-mb: {{ bk_stts.br_pd_mb }}px;--border-bg: {{ br_bg }};{%- endcapture -%}
                {%- endif -%}
                <{{ ARRhtml[0] }} {{ ARRhtml[1] }}href="{{ bk_stts.b_link }}" {{ ARRhtml[2] }}target="{{ bk_stts.open_link }}" class="t4s-d-block t4s_ratio t4s_ratio_hasmb">
                    {%- if image != blank -%}
                        {%- liquid
                            assign img_url_w     = image | image_url: width: 1904 | split: '1904'
                            assign img_url_w0    = img_url_w[0]
                            assign img_url_w1    = img_url_w[1]
                            assign img_url_w_mb  = mb_image | image_url: width: 1904 | split: '1904'
                            assign img_url_w_mb0 = img_url_w_mb[0]
                            assign img_url_w_mb1 = img_url_w_mb[1] 
                            assign img_1         = img_url_w0 | append: 1 | append: img_url_w1
                            assign img_1_mb      = img_url_w_mb0 | append: 1 | append: img_url_w_mb1
                            assign url_img       = 'url(' | append: img_1 | append: ')'
                            assign url_img_mb    = 'url(' | append: img_1_mb | append: ')'
                       -%}
                        <img {% if mb_image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ mb_image.presentation.focal_point }}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-img-as-bg t4s-d-md-none" data-src="{{ img_1_mb }}" data-widths="[375, 575, 750]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ mb_image.width }}" height="{{ mb_image.height }}" alt="{{ mb_image.alt | escape }}" loading="lazy">
                        <span class="lazyloadt4s-loader is-bg-img t4s-d-md-none" style="background: {{ url_img_mb }}"></span>
                        <img {% if image.presentation.focal_point != '50.0% 50.0%' %} style="object-position: {{ image.presentation.focal_point }}"{% endif %} class="lazyloadt4s t4s-lz--fadeIn t4s-obj-eff t4s-img-as-bg t4s-d-none t4s-d-md-block" data-src="{{ img_1 }}" data-widths="[800, 1000, 1200, 1400, 1600, 1800, 2000, 2200, 2500, 3000, 3400, 3800, 4100]" data-optimumx="1.5" data-sizes="auto" src="data:image/gif;base64,R0lGODlhAQABAAAAACH5BAEKAAEALAAAAAABAAEAAAICTAEAOw==" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                        <span class="lazyloadt4s-loader is-bg-img t4s-d-none t4s-d-md-block" style="background: {{ url_img }}"></span>
                    {%- else -%}
                    {{ 'image' | placeholder_svg_tag: 't4s-placeholder-svg t4s-svg-bg1 t4s-obj-eff' }}
                    {%- endif -%}
                </{{ ARRhtml[0] }}>
              <div class="t4s-banner-content t4s-content-position t4s-pa t4s-content-auto t4s-pe-none t4s-text-lg-{{ bk_stts.content_align }} t4s-text-md-{{ bk_stts.content_align_tb }} t4s-text-{{ bk_stts.content_align_mb }} t4s-bg-content-{{ bk_stts.bg_content_bl }} t4s-br-content-{{ bk_stts.border_bl }} t4s-br-style-{{ bk_stts.br_style }}" style="--time-animation: {{ bk_stts.time_animation }}s; {%- render 'content_position', ch_pos: bk_stts.ch_pos, cv_pos: bk_stts.cv_pos, ch_pos_tb: bk_stts.ch_pos_tb, cv_pos_tb: bk_stts.cv_pos_tb, ch_pos_mb: bk_stts.ch_pos_mb, cv_pos_mb: bk_stts.cv_pos_mb, append_bg_content_style: append_bg_content_style, append_bg_border_style: append_bg_border_style -%}">
        {%- else -%} 
            {%- liquid          
                if no_block_parent
                    continue
                endif
                assign bk_stts = block.settings 
           -%}
            {%- case block_type -%}
                {%- when 'custom_text' -%}
                    {%- if bk_stts.text != blank -%}
                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                        {%- assign general_block = true -%}
                        <{{ bk_stts.tag }} class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-text-bl t4s-fnt-fm-{{ bk_stts.fontf }} t4s-font-italic-{{ bk_stts.font_italic }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-hidden-br-tb-{{ bk_stts.remove_br_tag_tb }} t4s-hidden-br-mb-{{ bk_stts.remove_br_tag_mb }} t4s-text-shadow-{{ bk_stts.text_shadow }}" data-lh="{{ bk_stts.text_lh_mb }}" data-lh-md="{{ bk_stts.text_lh_tb }}" data-lh-lg="{{ bk_stts.text_lh }}" {%- render 'bk_cus_style', type: 'custom_text', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.text }}</{{ bk_stts.tag }}>
                    {%- endif -%}
                {%- when 'space_html' -%}
                    {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    {%- assign general_block = true -%}
                    <div class="t4s-space-html t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" {%- render 'bk_cus_style', type: 'space_html', bk_stts: bk_stts, ani_delay: ani_delay -%}></div>
                {%- when 'html' -%}
                    {%- if bk_stts.html_content != blank -%}
                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                        {%- assign general_block = true -%}
                            <div class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-raw-html t4s-rte--list t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" {%- render 'bk_cus_style', type: 'html', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.html_content }}</div>
                        {%- endif -%}
                {%- when 'image' -%}
                    {%- assign image = bk_stts.image_child -%}
                    {%- if image != blank -%}
                        {%- assign general_block = true -%}
                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%}{%- endif -%}
                        <div class="t4s-bl-item t4s-img-child t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" id="b_{{ block.id }}" {{ block.shopify_attributes }} {%- render 'bk_cus_style', type: 'image', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                            <img data-maxw="{{ bk_stts.img_width_mb }}" data-maxw-md="{{ bk_stts.img_width_tb }}" data-maxw-lg="{{ bk_stts.img_width }}" class="lazyloadt4s t4s-lz--fadeIn" data-src="{{ image | image_url: width: 1 }}" data-widths="[100,200,400,600,700,800,900,1000,1200,1400,1600]" data-optimumx="2" data-sizes="auto" src="{% render 'img_svg', w: image.width, h: image.height %}" width="{{ image.width }}" height="{{ image.height }}" alt="{{ image.alt | escape }}" loading="lazy">
                        </div>
                    {%- endif -%}
                {%- when "countdown" -%}
                        {%- if bk_stts.date != blank -%}
                            {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                            {%- assign countdown = true -%}
                            <div class="t4s-bl-item t4s-countdown t4s-countdown-des-{{ bk_stts.cdt_des }} t4s-countdown-size-{{ bk_stts.cdt_size }} t4s-animation-{{ bk_stts.animation }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }}" {%- render 'bk_cus_style', type: 'countdown', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                                <div class="t4s-time" data-countdown-t4s data-date='{{ bk_stts.date }}' data-keyid='#countdown-{{ sid }}'></div>
                            </div>
                        {%- endif -%}
                {%- when 'custom_button' -%}
                    {%- if bk_stts.button_link != blank and bk_stts.button_text != blank -%}
                        {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%}{%- endif -%}
                        {%- assign use_button = true -%}
                        {%- assign button_style = bk_stts.button_style -%}
                        <a href="{{ bk_stts.button_link }}" target="{{ bk_stts.target_link }}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-btn t4s-btn-custom t4s-pe-auto t4s-fnt-fm-{{ bk_stts.fontf }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-btn-style-{{ button_style }} {% if button_style == 'default' or button_style == 'outline' %}t4s-btn-effect-{{ bk_stts.button_effect }}{% endif %}" {%- render 'bk_cus_style', type: 'custom_button', bk_stts: bk_stts, ani_delay: ani_delay -%}>{{ bk_stts.button_text }}
                            {%- if bk_stts.button_icon_w > 0 -%}<svg  class="t4s-btn-icon" viewBox="0 0 14 10"><use xlink:href="#t4s-icon-btn"></use></svg>{%- endif -%}
                        </a>
                    {%- endif -%}
                {%- when 'newsletter' -%}
                    {%- if bk_stts.animation != 'none' -%}{%- assign use_animation = true -%} {%- endif -%}
                    {%- assign newsletter = true -%}
                    <div id="b_{{ block.id }}" class="t4s-bl-item t4s-animation-{{ bk_stts.animation }} t4s-pe-auto t4s-newsletter-parent  t4s-newsl-des-{{ bk_stts.newl_des }} t4s-newsl-{{ bk_stts.newl_size }} t4s-hidden-mobile-{{ bk_stts.hidden_mobile }} t4s-text-start" {%- render 'bk_cus_style', type: 'newsletter', bk_stts: bk_stts, ani_delay: ani_delay -%}>
                        {%- render 'newsletter', form_id: block.id, buttonIcon: bk_stts.btn_icon, bk_stts: bk_stts -%}
                    </div>
            {%- endcase -%}
            {%- if bk_stts.animation != 'none' -%}{%- assign ani_delay = ani_delay | plus: time_ani_delay -%}{%- endif -%}
        {%- endif -%}
        {%- liquid
            if forloop.last
                echo '</div></div></div></div>'
            endif -%}
    {%- endfor -%}
  </div>
  {{- html_layout[1] -}}
</div> 
{%- if general_block -%}
  {{ 't4s-general-block.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if use_button -%}
  {{ 't4s-button-style.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if use_animation -%}
    <link href="{{ 't4s-animation.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
    <link href="{{ 't4s-custom-effect.css' | asset_url }}" rel="stylesheet" media="print" onload="this.media='all'">
{%- endif -%}
{%- if newsletter -%}
  {{ 't4s-newsletter.css' | asset_url | stylesheet_tag }}
{%- endif -%}
{%- if countdown -%} 
    {{ 't4s-countdown.css' | asset_url | stylesheet_tag }}
    <template id="countdown-{{ sid }}">
        <span class="t4s-countdown-days">
            <span class="cd_timet4s t4s-cd-number">%-D</span>
            <span class="cd_txtt4s t4s-cd-text">%!D:{{ "sections.countdown_text.day" | t }},{{ "sections.countdown_text.day_plural" | t }};</span>
        </span>
        <span class="t4s-countdown-hours">
            <span class="cd_timet4s t4s-cd-number">%H</span> 
            <span class="cd_txtt4s t4s-cd-text">%!H:{{ "sections.countdown_text.hr" | t }},{{ "sections.countdown_text.hr_plural" | t }};</span>
        </span>
        <span class="t4s-countdown-min">
            <span class="cd_timet4s t4s-cd-number">%M</span> 
            <span class="cd_txtt4s t4s-cd-text">%!M:{{ "sections.countdown_text.min" | t }},{{ "sections.countdown_text.min_plural" | t }};</span>
        </span>
        <span class="t4s-countdown-sec">
            <span class="cd_timet4s t4s-cd-number">%S</span> 
            <span class="cd_txtt4s t4s-cd-text">%!S:{{ "sections.countdown_text.sec" | t }},{{ "sections.countdown_text.sec_plural" | t }};</span>
        </span>
    </template>
{%- endif -%}
